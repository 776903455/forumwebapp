    <select id="selectArtByTime"  resultMap="findArtByCsidMap">
        select a.aid,a.aname,a.looknum,u.username from tbl_article a LEFT JOIN tbl_user u ON a.uid=u.uid
        <where>
            <if test="timevalue==0">
                and a.csid=#{csid} ORDER BY adate desc
